/* pages/detail/detail.wxss */

page {
  height: 100%;
  overflow: hidden;
}

.record {
  padding: 5rpx 20rpx;
  /* border: 1px solid #000; */
  border-radius: 24rpx;
  position: absolute;
  z-index: 5000;
  font-size: 24rpx;
  color: #fff;
  top: 10rpx;
  right: 10rpx;
  box-shadow: 0 0 5px 1rpx inset, 0 0 1rpx 1rpx rgba(0, 0, 0, 0.5);
  background: rgb(219, 50, 50);
}

.container {
  height: 100%;
  overflow: hidden;
}

.bg-img {
  position: absolute;
  left: 0;
  top: 0;
  width: 750rpx;
  height: 100%;
}

.slogo {
  position: absolute;
  left: 0;
  top: 160rpx;
  width: 750rpx;
  height: 120rpx;
}

.red-paper {
  position: absolute;
  left: 50%;
  top: 260rpx;
  transform: translateX(-50%);
  width: 379rpx;
  height: 464rpx;
}

.paper-desc {
  position: absolute;
  top: 720rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
}

.red-paper-btn {
  position: absolute;
  left: 50%;
  top: 380rpx;
  transform: translateX(-50%);
  width: 170rpx;
  height: 170rpx;
}

.member {
  display: flex;
  justify-content: space-around;
  position: absolute;
  left: 0;
  top: 780rpx;
  width: 750rpx;
  height: 120rpx;
}

.avatar {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
}

.nickname {
  margin-top: 20rpx;
  font-size: 20rpx;
  text-align: center;
  color: #fff;
  overflow: hidden;
  width: 96rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.avatar-bg {
  border: solid 1px #dcc721;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: contain;
}

.tip {
  position: absolute;
  bottom: 190rpx;
  text-align: center;
  color: #fff;
  font-size: 40rpx;
}

.gold-font {
  color: #dcc721;
  font-weight: bold;
}

.invite {
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: absolute;
  bottom: 80rpx;
}

.invite-btn {
  width: 375rpx;
  height: 95rpx;
}

.rule {
  position: absolute;
  bottom: 15rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  text-decoration: underline;
}
